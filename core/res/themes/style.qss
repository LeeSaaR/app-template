QLabel[accessibleDescription="item_month"] {background-color:#303030; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QLabel[accessibleDescription="item_pay"] {background-color:#303030; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QPushButton[accessibleDescription="item_pay"] {border: none; background-color:#303030; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}

QPushButton[accessibleDescription="item_time_flat"] {border: none; background-color: #283042; }
QPushButton[accessibleDescription="item_time_flat"]:checked {background-color: #303030;}
QPushButton[accessibleDescription="item_time_flat"]:disabled {background-color: #403535;}

QPushButton[accessibleDescription="item_time_flat_party"] {border: none; background-color: #252525; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QPushButton[accessibleDescription="item_time_flat_party"]:checked {background-color: #303030;}

QPushButton[accessibleDescription="item_time_void"] {border: none; background-color: #252525; }
QPushButton[accessibleDescription="item_time_void"]:checked {background-color: #283042;}


QPushButton[accessibleDescription="item_time_party"] {border: none; background-color: #252525; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QPushButton[accessibleDescription="item_time_party"]:checked {background-color: #303030;}
QLabel[accessibleDescription="item_pay"]:disabled,QPushButton[accessibleDescription="item_time_party"]:disabled {background-color: transparent;}

QComboBox[accessibleDescription="cbox"] {border: none; background-color: #303030; color: #ffffff; selection-background-color: #404040; selection-color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QComboBox[accessibleDescription="cbox"]:hover{ border: none; background-color: #414855;}
QComboBox[accessibleDescription="cbox"]::down-arrow {border: none; image: url(); border-width: 0px; width:0px;}
QComboBox[accessibleDescription="cbox"]::drop-down {border: none; color: #ffffff; subcontrol-origin: padding; subcontrol-position: top right; border-width: 0px;}
QComboBox[accessibleDescription="cbox"]::indicator{border: none; width:0px; background-color:transparent; color:transparent; selection-background-color:transparent; selection-color:transparent;}
QComboBox[accessibleDescription="cbox"] QAbstractItemView{border: none; background-color: #303030; color: #ffffff;}

QPushButton[accessibleDescription="wgt_list_item"] {text-align: left; padding-left: 8px; border: none; background-color: #292a2b; color: #ffffff; font-size: <FONTSIZE_DEFAULT>;}
QPushButton[accessibleDescription="wgt_list_item"]:hover {background-color: #2c2d2e;}
QPushButton[accessibleDescription="wgt_list_item"]:checked {background-color: #3e4042;}
QPushButton[accessibleDescription="wgt_list_item"]:checked:hover {background-color: #3e4042;}
QPushButton[accessibleDescription="wgt_list_item"]:pressed {background-color: #404040;}


QFrame [accessibleDescription="tabbar_control"] {
    background-color: #3e4042;
    border: none;
}

QTabWidget::tab-bar {
    border: none;
}

QTabBar::tab:selected, QTabBar::tab:!selected:hover {
    border: none;
    font-size: <FONTSIZE_DEFAULT>;
    background-color: #3e4042;
    color: #ffffff;
    height: 24px;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 8px;
    padding-right: 8px;
    margin-right: 2px;
    min-width: 130px;
}

QTabBar::tab:!selected {
    border: none;
    font-size: <FONTSIZE_DEFAULT>;
    background-color: #292a2b;
    color: #ffffff;
    height: 24px;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 8px;
    padding-right: 8px;
    margin-right: 2px;
    min-width: 130px;
}

QTabBar::close-button {
    border: none;
    background-color: transparent;
    image: url(":/icons/close_big.svg");
    height: 24px;
    width: 24px;
}
QTabBar::close-button:hover {
    background-color: #606060;
    image: url(":/icons/close_big.svg");
    height: 24px;
    width: 24px;
}