QCheckBox {
height: 30px;
spacing: 10px;
background-color: none;
color: #eff4fc;
font-size: <FONTSIZE_DEFAULT>;}
QCheckBox::indicator{
background: none;
width: 14px;
height: 14px;}
QCheckBox::indicator:checked{
width: 6px;
height: 6px;
background-color: #3d4041;
border: 6px solid <COLOR_MAIN>;
border-radius: 9px;}
QCheckBox::indicator:unchecked{
width: 16px;
height: 16px;
background-color: #3d4041;
border: 1px solid <COLOR_MAIN>;
border-radius: 8px;}
QCheckBox::indicator:checked:hover{
border: 6px solid #94a8cc;}
QCheckBox::indicator:unchecked:hover{
background-color: #626f87;}

QRadioButton[accessibleName="normal"]{
height: 30px;
spacing: 10px;
background-color: none;
color: #eff4fc;
font-size: <FONTSIZE_DEFAULT>;}
QRadioButton::indicator{
background: none;
width: 14px;
height: 14px;}
QRadioButton[accessibleName="normal"]::indicator:checked{
width: 6px;
height: 6px;
background-color: #3d4041;
border: 6px solid <COLOR_MAIN>;
border-radius: 9px;}
QRadioButton[accessibleName="normal"]::indicator:unchecked{
width: 16px;
height: 16px;
background-color: #3d4041;
border: 1px solid <COLOR_MAIN>;
border-radius: 8px;}
QRadioButton[accessibleName="normal"]::indicator:unchecked:hover{
background-color: #626f87;}
